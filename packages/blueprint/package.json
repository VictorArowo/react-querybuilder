{
  "name": "@react-querybuilder/blueprint",
  "description": "Custom Blueprint components for react-querybuilder",
  "version": "6.5.3",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/react-querybuilder_blueprint.legacy-esm.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/react-querybuilder_blueprint.d.mts",
        "default": "./dist/react-querybuilder_blueprint.mjs"
      },
      "require": {
        "types": "./dist/cjs/react-querybuilder_blueprint.cjs.development.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "types": "./dist/react-querybuilder_blueprint.d.mts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/react-querybuilder/react-querybuilder.git",
    "directory": "packages/blueprint"
  },
  "license": "MIT",
  "homepage": "https://react-querybuilder.js.org/",
  "scripts": {
    "start": "vite",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "devDependencies": {
    "@blueprintjs/core": "^5.5.1",
    "@blueprintjs/datetime": "^5.2.4",
    "@blueprintjs/datetime2": "^2.2.2",
    "@blueprintjs/icons": "^5.2.1",
    "@blueprintjs/select": "^5.0.16",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.31",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "dayjs": "^1.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-querybuilder": "^6.5.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "peerDependencies": {
    "@blueprintjs/core": ">=5",
    "@blueprintjs/datetime": ">=5",
    "@blueprintjs/datetime2": ">=2",
    "@blueprintjs/icons": ">=5",
    "@blueprintjs/select": ">=5",
    "dayjs": ">=1",
    "react": ">=16.8.0",
    "react-querybuilder": "^6.5.3"
  }
}
